{
	"name": "levita-js",
	"version": "0.1.1",
	"description": "Lightweight 3D tilt & parallax with accelerometer support",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./style.css": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"build": "tsdown && cp src/style.css dist/style.css",
		"dev": "tsdown --watch"
	},
	"keywords": [
		"tilt",
		"parallax",
		"3d",
		"accelerometer",
		"gyroscope",
		"css",
		"vanilla"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeromearsene/levita.git",
		"directory": "packages/core"
	},
	"homepage": "https://github.com/jeromearsene/levita#readme",
	"devDependencies": {
		"tsdown": "^0.20.3"
	}
}
